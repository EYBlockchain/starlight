from "./common/hashes/mimc/altbn254/mimc2.zok" import main as mimc2
from "hashes/sha256/sha256Padded.zok" import sha256Padded as sha256Padded
from "utils/casts/u32_8_to_bool_256.zok" import main as u32_8_to_bool_256
from "hashes/poseidon/poseidon.zok" import main as poseidon
from "utils/pack/bool/nonStrictUnpack256.zok" import main as field_to_bool_256
from "./common/casts/u32_array_to_field.zok" import main as u32_array_to_field
from "utils/pack/bool/pack256.zok" import main as bool_256_to_field
from "./common/merkle-tree/mimc/altbn254/verify-membership/height32.zok" import main as checkRoot

def main(\
	private field accountId,\
	private field amountDeposit,\
	private u32[8] account_accountId_oldCommitment_owner_secretKey,\
	public field account_accountId_oldCommitment_nullifier,\
	private  field account_accountId_oldCommitment_value,\
	private field account_accountId_oldCommitment_salt_field,\
	private bool account_accountId_oldCommitment_isDummy,\
	public field commitmentRoot,\
	private field account_accountId_oldCommitment_membershipWitness_index,\
	private field[32] account_accountId_oldCommitment_membershipWitness_siblingPath,\
	private field account_accountId_newCommitment_owner_publicKey_field,\
	private field account_accountId_newCommitment_salt_field,\
	public field account_accountId_newCommitment_commitment\
) -> (bool):

        
        // We need to hard-code the mappingId's of mappings into the circuit:
        field account_mappingId = 5


        field account_accountId = account_accountId_oldCommitment_value

account_accountId = account_accountId + amountDeposit


        field account_accountId_stateVarId_field = mimc2([account_mappingId, accountId])


        // account_accountId_oldCommitment - PoKoSK:
        // The correctness of this secret key will be constrained within the oldCommitment existence check.

        u32[8] account_accountId_oldCommitment_owner_publicKey = sha256Padded(u32_8_to_bool_256(account_accountId_oldCommitment_owner_secretKey))


        // Nullify account_accountId:

        field account_accountId_oldCommitment_owner_secretKey_field =u32_array_to_field(account_accountId_oldCommitment_owner_secretKey)

        field account_accountId_oldCommitment_nullifier_check_field = poseidon([\
          account_accountId_stateVarId_field,\
          account_accountId_oldCommitment_owner_secretKey_field,\
          account_accountId_oldCommitment_salt_field\
        ])

        assert(\
        field_to_bool_256(account_accountId_oldCommitment_nullifier)[8..256] == field_to_bool_256(account_accountId_oldCommitment_nullifier_check_field)[8..256]\
        )


        // account_accountId_oldCommitment_commitment: preimage check

        field account_accountId_oldCommitment_owner_publicKey_field =u32_array_to_field(account_accountId_oldCommitment_owner_publicKey)


        field account_accountId_oldCommitment_commitment_field = poseidon([\
          account_accountId_stateVarId_field,\
          account_accountId_oldCommitment_value,\
          account_accountId_oldCommitment_owner_publicKey_field,\
          account_accountId_oldCommitment_salt_field\
        ])


        // account_accountId_oldCommitment_commitment: existence check


        field account_accountId_commitmentRoot_check = checkRoot(\
          account_accountId_oldCommitment_membershipWitness_siblingPath,\
          account_accountId_oldCommitment_commitment_field,\
          account_accountId_oldCommitment_membershipWitness_index\
        )


        // Note: Don't bother actually asserting existence, if the oldCommitment is a dummy:
        account_accountId_commitmentRoot_check = if account_accountId_oldCommitment_isDummy == true then commitmentRoot else account_accountId_commitmentRoot_check fi


        assert(\
          field_to_bool_256(commitmentRoot)[8..256] == field_to_bool_256(account_accountId_commitmentRoot_check)[8..256]\
        )


        // prepare secret state 'account_accountId' for commitment

        field account_accountId_newCommitment_value_field = account_accountId

        // account_accountId_newCommitment_commitment - preimage check

        field account_accountId_newCommitment_commitment_check_field = poseidon([\
          account_accountId_stateVarId_field,\
          account_accountId_newCommitment_value_field,\
          account_accountId_newCommitment_owner_publicKey_field,\
          account_accountId_newCommitment_salt_field\
        ])

        assert(\
          field_to_bool_256(account_accountId_newCommitment_commitment)[8..256] == field_to_bool_256(account_accountId_newCommitment_commitment_check_field)[8..256]\
        )

         return true
        