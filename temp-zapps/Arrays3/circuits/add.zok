from "./common/hashes/mimc/altbn254/mimc2.zok" import main as mimc2
from "ecc/babyjubjubParams" import main as curveParams
from "ecc/edwardsScalarMult" import main as scalarMult
from "ecc/edwardsCompress" import main as edwardsCompress
from "utils/pack/bool/pack256.zok" import main as bool_256_to_field
from "utils/pack/bool/nonStrictUnpack256.zok" import main as field_to_bool_256
from "hashes/poseidon/poseidon.zok" import main as poseidon
from "./common/merkle-tree/sparse-merkle-tree/checkproof.zok" import main as checkproof
from "./common/merkle-tree/sparse-merkle-tree/checkproof.zok" import checkUpdatedPath as checkUpdatedPath
from "./common/merkle-tree/mimc/altbn254/verify-membership/height32.zok" import main as checkRoot

def main(\
	public field index,\
	private field y_index_1_oldCommitment_owner_secretKey,\
	public field nullifierRoot,\
	public field newNullifierRoot,\
	public field y_index_1_oldCommitment_nullifier,\
	private field[32] y_index_1_nullifier_nonmembershipWitness_siblingPath,\
	private field[32] y_index_1_nullifier_nonmembershipWitness_newsiblingPath,\
	private  field y_index_1_oldCommitment_value,\
	private field y_index_1_oldCommitment_salt,\
	private bool y_index_1_oldCommitment_isDummy,\
	public field commitmentRoot,\
	private field y_index_1_oldCommitment_membershipWitness_index,\
	private field[32] y_index_1_oldCommitment_membershipWitness_siblingPath,\
	private field y_index_1_newCommitment_owner_publicKey,\
	private field y_index_1_newCommitment_salt,\
	public field y_index_1_newCommitment_commitment\
) -> (bool):

        
        // We need to hard-code the mappingId's of mappings into the circuit:
        field y_mappingId = 6


        field y_index_1 = y_index_1_oldCommitment_value


        field index_init = index


        field index_1 = index + 1

index = index_1

y_index_1 = y_index_1 + 1

index = index_init


        field y_index_1_stateVarId_field = mimc2([y_mappingId, index_1])


        // y_index_1_oldCommitment - PoKoSK:
        // The correctness of this secret key will be constrained within the oldCommitment existence check.

        field[2] y_index_1_oldCommitment_owner_publicKey_point = scalarMult(field_to_bool_256(y_index_1_oldCommitment_owner_secretKey), [curveParams().Gu, curveParams().Gv], curveParams())

        bool y_index_1_oldCommitment_owner_publicKey_sign = edwardsCompress(y_index_1_oldCommitment_owner_publicKey_point)[0]

        bool[254] y_index_1_oldCommitment_yBits = field_to_bool_256(y_index_1_oldCommitment_owner_publicKey_point[1])[2..256]
        y_index_1_oldCommitment_yBits[0] = y_index_1_oldCommitment_owner_publicKey_sign

        field y_index_1_oldCommitment_owner_publicKey = bool_256_to_field([false, false, ...y_index_1_oldCommitment_yBits])


        // Nullify y_index_1:

        field y_index_1_oldCommitment_nullifier_check_field = poseidon([\
          y_index_1_stateVarId_field,\
          y_index_1_oldCommitment_owner_secretKey,\
          y_index_1_oldCommitment_salt\
        ])

        assert(\
        field_to_bool_256(y_index_1_oldCommitment_nullifier)[8..256] == field_to_bool_256(y_index_1_oldCommitment_nullifier_check_field)[8..256]\
        )
        // y_index_1_oldCommitment_nullifier : non-existence check
        
        assert(\
          nullifierRoot == checkproof(\
            y_index_1_nullifier_nonmembershipWitness_siblingPath,\
            y_index_1_oldCommitment_nullifier\
           )       )

       assert(\
        newNullifierRoot == checkUpdatedPath(\
          y_index_1_nullifier_nonmembershipWitness_newsiblingPath,\
          y_index_1_oldCommitment_nullifier\
        )        )

        



        // prepare secret state 'y_index_1' for commitment

          field y_index_1_oldCommitment_value_field = y_index_1_oldCommitment_value
          
        // y_index_1_oldCommitment_commitment: preimage check


        field y_index_1_oldCommitment_commitment_field = poseidon([\
          y_index_1_stateVarId_field,\
          y_index_1_oldCommitment_value_field,\
          y_index_1_oldCommitment_owner_publicKey,\
          y_index_1_oldCommitment_salt        ])


        // y_index_1_oldCommitment_commitment: existence check


        field y_index_1_commitmentRoot_check = checkRoot(\
          y_index_1_oldCommitment_membershipWitness_siblingPath,\
          y_index_1_oldCommitment_commitment_field,\
          y_index_1_oldCommitment_membershipWitness_index\
        )


        // Note: Don't bother actually asserting existence, if the oldCommitment is a dummy:
        y_index_1_commitmentRoot_check = if y_index_1_oldCommitment_isDummy == true then commitmentRoot else y_index_1_commitmentRoot_check fi


        assert(\
          field_to_bool_256(commitmentRoot)[8..256] == field_to_bool_256(y_index_1_commitmentRoot_check)[8..256]\
        )


        // prepare secret state 'y_index_1' for commitment

        field y_index_1_newCommitment_value_field = y_index_1

        // y_index_1_newCommitment_commitment - preimage check

        field y_index_1_newCommitment_commitment_check_field = poseidon([\
          y_index_1_stateVarId_field,\
          y_index_1_newCommitment_value_field,\
          y_index_1_newCommitment_owner_publicKey,\
          y_index_1_newCommitment_salt\
        ])

        assert(\
          field_to_bool_256(y_index_1_newCommitment_commitment)[8..256] == field_to_bool_256(y_index_1_newCommitment_commitment_check_field)[8..256]\
        )

         return true