from "./common/hashes/mimc/altbn254/mimc2.zok" import main as mimc2
from "hashes/sha256/sha256Padded.zok" import sha256Padded as sha256Padded
from "utils/casts/u32_8_to_bool_256.zok" import main as u32_8_to_bool_256
from "hashes/poseidon/poseidon.zok" import main as poseidon
from "utils/pack/bool/nonStrictUnpack256.zok" import main as field_to_bool_256
from "./common/casts/u32_array_to_field.zok" import main as u32_array_to_field
from "utils/pack/bool/pack256.zok" import main as bool_256_to_field
from "./common/merkle-tree/mimc/altbn254/verify-membership/height32.zok" import main as checkRoot

def main(\
	private field param2,\
	public field key,\
	private u32[8] b_key_0_oldCommitment_owner_secretKey,\
	private u32[8] b_key_1_oldCommitment_owner_secretKey,\
	public field b_key_0_oldCommitment_nullifier,\
	public field b_key_1_oldCommitment_nullifier,\
	private  field b_key_0_oldCommitment_value,\
	private field b_key_0_oldCommitment_salt_field,\
	private  field b_key_1_oldCommitment_value,\
	private field b_key_1_oldCommitment_salt_field,\
	public field commitmentRoot,\
	private field b_key_0_oldCommitment_membershipWitness_index,\
	private field[32] b_key_0_oldCommitment_membershipWitness_siblingPath,\
	private field b_key_1_oldCommitment_membershipWitness_index,\
	private field[32] b_key_1_oldCommitment_membershipWitness_siblingPath,\
	private field b_key_2_newCommitment_owner_publicKey_field,\
	private field b_key_2_newCommitment_salt_field,\
	public field b_key_2_newCommitment_commitment\
) -> (bool):

        
        // We need to hard-code the mappingId's of mappings into the circuit:
        field b_mappingId = 7


        field b_key_0 = b_key_0_oldCommitment_value


        field b_key_1 = b_key_1_oldCommitment_value

// Moved decrementation of b_key


        field b_key_0_stateVarId_field = mimc2([b_mappingId, key])


        field b_key_1_stateVarId_field = mimc2([b_mappingId, key])


        field b_key_2_stateVarId_field = mimc2([b_mappingId, key])


        // b_key_0_oldCommitment - PoKoSK:
        // The correctness of this secret key will be constrained within the oldCommitment existence check.

        u32[8] b_key_0_oldCommitment_owner_publicKey = sha256Padded(u32_8_to_bool_256(b_key_0_oldCommitment_owner_secretKey))


        // b_key_1_oldCommitment - PoKoSK:
        // The correctness of this secret key will be constrained within the oldCommitment existence check.

        u32[8] b_key_1_oldCommitment_owner_publicKey = sha256Padded(u32_8_to_bool_256(b_key_1_oldCommitment_owner_secretKey))


        // Nullify b_key_0:

        field b_key_0_oldCommitment_owner_secretKey_field =u32_array_to_field(b_key_0_oldCommitment_owner_secretKey)

        field b_key_0_oldCommitment_nullifier_check_field = poseidon([\
          b_key_0_stateVarId_field,\
          b_key_0_oldCommitment_owner_secretKey_field,\
          b_key_0_oldCommitment_salt_field\
        ])

        assert(\
        field_to_bool_256(b_key_0_oldCommitment_nullifier)[8..256] == field_to_bool_256(b_key_0_oldCommitment_nullifier_check_field)[8..256]\
        )


        // Nullify b_key_1:

        field b_key_1_oldCommitment_owner_secretKey_field =u32_array_to_field(b_key_1_oldCommitment_owner_secretKey)

        field b_key_1_oldCommitment_nullifier_check_field = poseidon([\
          b_key_1_stateVarId_field,\
          b_key_1_oldCommitment_owner_secretKey_field,\
          b_key_1_oldCommitment_salt_field\
        ])

        assert(\
        field_to_bool_256(b_key_1_oldCommitment_nullifier)[8..256] == field_to_bool_256(b_key_1_oldCommitment_nullifier_check_field)[8..256]\
        )


        // b_key_0_oldCommitment_commitment: preimage check

        field b_key_0_oldCommitment_owner_publicKey_field =u32_array_to_field(b_key_0_oldCommitment_owner_publicKey)


        field b_key_0_oldCommitment_commitment_field = poseidon([\
          b_key_0_stateVarId_field,\
          b_key_0_oldCommitment_value,\
          b_key_0_oldCommitment_owner_publicKey_field,\
          b_key_0_oldCommitment_salt_field\
        ])


        // b_key_1_oldCommitment_commitment: preimage check

        field b_key_1_oldCommitment_owner_publicKey_field =u32_array_to_field(b_key_1_oldCommitment_owner_publicKey)


        field b_key_1_oldCommitment_commitment_field = poseidon([\
          b_key_1_stateVarId_field,\
          b_key_1_oldCommitment_value,\
          b_key_1_oldCommitment_owner_publicKey_field,\
          b_key_1_oldCommitment_salt_field\
        ])


        // b_key_0_oldCommitment_commitment: existence check


        field b_key_0_commitmentRoot_check = checkRoot(\
          b_key_0_oldCommitment_membershipWitness_siblingPath,\
          b_key_0_oldCommitment_commitment_field,\
          b_key_0_oldCommitment_membershipWitness_index\
        )


        assert(\
          field_to_bool_256(commitmentRoot)[8..256] == field_to_bool_256(b_key_0_commitmentRoot_check)[8..256]\
        )


        // b_key_1_oldCommitment_commitment: existence check


        field b_key_1_commitmentRoot_check = checkRoot(\
          b_key_1_oldCommitment_membershipWitness_siblingPath,\
          b_key_1_oldCommitment_commitment_field,\
          b_key_1_oldCommitment_membershipWitness_index\
        )


        assert(\
          field_to_bool_256(commitmentRoot)[8..256] == field_to_bool_256(b_key_1_commitmentRoot_check)[8..256]\
        )


        // prepare secret state 'b_key_2' for commitment

        assert(b_key_0 + b_key_1 > param2)
            // TODO: assert no under/overflows

            field b_key_2_newCommitment_value_field = (b_key_0 + b_key_1) - (param2)

        // b_key_2_newCommitment_commitment - preimage check

        field b_key_2_newCommitment_commitment_check_field = poseidon([\
          b_key_2_stateVarId_field,\
          b_key_2_newCommitment_value_field,\
          b_key_2_newCommitment_owner_publicKey_field,\
          b_key_2_newCommitment_salt_field\
        ])

        assert(\
          field_to_bool_256(b_key_2_newCommitment_commitment)[8..256] == field_to_bool_256(b_key_2_newCommitment_commitment_check_field)[8..256]\
        )

         return true
        