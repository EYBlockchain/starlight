pragma solidity ^0.8.0;

contract MyContract {

  secret mapping(address => uint256) private a;

  function increment(secret uint256 param2) public {
    unknown a[msg.sender] += param2;
  }

  function decrement(secret uint256 param3) public {
    a[msg.sender] -= param3;
  }

}
