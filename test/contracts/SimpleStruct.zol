pragma solidity ^0.8.0;

contract Assign {

  secret uint256 private a;

  struct MyStruct {
    uint256 prop1;
    bool prop2;
  }

  MyStruct public x;

  function add(MyStruct memory value) public {
    unknown a += value.prop1;
    x.prop2 = true;
  }
  function remove(MyStruct memory value) public {
     a -= value.prop1;
    x.prop2 = true;
  }
}
