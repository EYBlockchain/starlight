pragma solidity ^0.8.0;

contract Assign {

  secret uint256 private a;
  uint256 public b;

  function add( uint256 value) public  returns (bool, uint256) {
    known a += value;
    return (true, a);
  }

  function remove(secret uint256 value, uint256 value1) public returns (uint256, uint256) {
    a -= value;
    b += value1;
    return (b, a);
  }
}
