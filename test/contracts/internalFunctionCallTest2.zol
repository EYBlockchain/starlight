pragma solidity ^0.8.0;

contract Assign {

  secret uint256 private a;
  secret uint256 private b;

  function addB(secret uint256 value) public {
    known b += value;

  }

  function addA(secret uint256 value) public {
    known a += value;
  }
  function remove(secret uint256 value) public {
    a -= value;
    addB(value);
    addA(value);


  }
}
