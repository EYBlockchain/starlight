pragma solidity ^0.8.0;
contract Assign {
secret uint256 private a;
secret uint256 private b;
 uint256 public index;
uint256 [3] public c;

function add( uint256 value) public {   
c[index] += value;
}

function add1( uint256 value) public {
c[index] += value;

}

function add2( uint256 value) public {
c[index] += value;
known a += value;
}

}