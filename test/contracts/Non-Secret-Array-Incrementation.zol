pragma solidity ^0.8.0;

contract Assign {
secret uint256 private a;
secret uint256 private b;
mapping(uint256 => uint256) public c;

function add( uint256 value ) public {
a+= value;
unknown b += value +a;
c[0] += value;
}

function add1(secret uint256 value ) public {
unknown b += value +a;
unknown b += c[0];
}

}