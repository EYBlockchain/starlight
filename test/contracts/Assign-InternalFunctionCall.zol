pragma solidity ^0.8.0;

contract Bank {

    secret mapping(uint256 => uint256) public buckets;

    function deposit(uint256 accountId, uint amountDeposit) public {
        unknown buckets[accountId] += amountDeposit;
    }

    function transfer(secret uint256 fromAccountId, secret uint256 toAccountId, secret uint256 amount) public {
        buckets[fromAccountId] -= amount;
        deposit(toAccountId, amount);
    }
}
