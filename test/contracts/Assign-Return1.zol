pragma solidity ^0.8.0;

contract Assign {

  secret uint256 private a;
  secret uint256 private b;
  
  function add( secret uint256 value) public  returns (uint256) {
    a += value;
    b += value;
    return b;
  }

  function remove(secret uint256 value) public {
    secret uint256 value1 = add(value);
    a -= value + value1;
  }
}
