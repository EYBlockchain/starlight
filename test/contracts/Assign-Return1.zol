pragma solidity ^0.8.0;

contract Assign {

  secret uint256 private a;

  function add( uint256 value) public  returns (bool) {
    a += value;
    return true;
  }

  function remove(secret uint256 value) public returns (uint256) {
    a -= value;
    return 5;
  }
}
