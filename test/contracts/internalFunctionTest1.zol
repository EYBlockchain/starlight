pragma solidity ^0.8.0;

contract MyContract {

  secret mapping(address => uint256) private a;

  function increment(secret uint256 param1, secret uint256 param2) public {
    unknown a[msg.sender] += param1;
    decrement(param2);


  }

  function decrement(secret uint256 param3, secret uint256 param4)) public {
    a[msg.sender] -= param3;
    increment(param4);
  }

}
