#!/bin/sh
set -e

docker-compose -f docker-compose.ganache.yml down -v

sleep 5

docker-compose -f docker-compose.ganache.yml up -d ganache
docker-compose -f docker-compose.ganache.yml up -d zokrates

sleep 5

docker-compose -f docker-compose.ganache.yml up -d deployer

sleep 15

docker-compose -f docker-compose.ganache.yml up -d timber

sleep 5

docker-compose -f docker-compose.ganache.yml up -d zapp
